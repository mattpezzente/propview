{
  "extends": "airbnb-base",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-vars": 2,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "spaced-comment": 0,
    "prefer-template": 0,
    "max-len": 0,
    "no-mixed-operators": 0,
    "no-alert": 0,
    "no-console": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}